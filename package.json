{
  "name": "synths-sdk",
  "version": "1.0.9",
  "description": "Yam Uma Synths SDK",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "author": "e, byterose",
  "contributors": [
    "snake"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:abi && hardhat compile --force && tsc",
    "build:abi": "typechain --target=ethers-v5 src/abi/*.json --out-dir src/types/abi",
    "test": "hardhat test",
    "test:w": "hardhat watch test",
    "format": "prettier --write .",
    "cover": "nyc yarn test",
    "cover:sol": "HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "lint": "yarn eslint --fix .",
    "lint:report": "yarn eslint --ext .ts --output-file eslint_report.json --format json .",
    "docgen": "yarn build && yarn typedoc && yarn run hardhat docgen"
  },
  "keywords": [
    "yam",
    "synths",
    "degenerative",
    "uma",
    "finance",
    "synthetics",
    "assets",
    "protocol"
  ],
  "dependencies": {
    "@openzeppelin/contracts": "^4.2.0",
    "@uma/core": "^2.10.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "prb-math": "^2.2.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.21",
    "@types/eslint": "^7.28.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.6",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "3.4.0",
    "hardhat": "^2.5.0",
    "hardhat-deploy": "^0.9.3",
    "hardhat-docgen": "^1.1.2",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typechain": "^5.1.2",
    "typedoc": "^0.22.6",
    "typescript": "4.4.3"
  }
}
